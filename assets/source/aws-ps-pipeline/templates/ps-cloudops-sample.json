{
  "Name": "ps-cloudops",
  "Comment": "Sample permission set for cloudops access",
  "Description": "Billing access in AWS",
  "SessionDuration": "PT8H",
  "ManagedPolicies": [
    "arn:aws:iam::aws:policy/ReadOnlyAccess"
  ],
  "CustomerPolicies": [],
  "CustomPolicy": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "IAMAccess",
        "Effect": "Allow",
        "Action": [
          "iam:List**",
          "iam:Get*",
          "iam:Generate*",
          "iam:Delete*",
          "iam:Attach*",
          "iam:Detach*",
          "iam:Set*",
          "iam:Update*",
          "iam:CreatePolicy",
          "iam:CreateInstanceProfile",
          "iam:CreateServiceLinkedRole",
          "iam:CreateVirtualMFADevice",
          "iam:CreatePolicyVersion",
          "access-analyzer:*"
        ],
        "Resource": "*"
      }
    ]
  },
  "PermissionBoundary": {
    "ManagedPolicy": "",
    "CustomerPolicy": ""
  },
  "Assignments": [
    {
      "all_accounts": false,
      "principal": "AWSControlTowerAdmins",
      "type": "GROUP",
      "account_id": [],
      "account_ou": ["ou-u5c1-65ade07l", "ou-u5c1-21e4hjq5"],
      "account_tag": []
    }
  ]
}